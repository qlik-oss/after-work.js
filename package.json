{
  "name": "after-work.js",
  "version": "4.1.0",
  "description": "Unified testing framework",
  "author": "QlikTech International AB",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src test examples",
    "lint:fix": "eslint src test --fix",
    "test": "./src/cli.js",
    "test:coverage": "./src/cli.js -c aw.config.js --coverage",
    "test:coveralls": "npm run test:coverage && cat coverage/lcov.info | coveralls",
    "version:next": "./scripts/next.js && npm publish --tag next && git checkout -- package.json"
  },
  "bin": {
    "aw": "./src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:qlik-oss/after-work.js.git"
  },
  "files": [
    "aw.png",
    "/src",
    "/docs"
  ],
  "dependencies": {
    "chai": "4.1.1",
    "chai-as-promised": "7.1.1",
    "chai-subset": "1.6.0",
    "chokidar": "2.0.2",
    "chrome-launcher": "0.10.2",
    "chrome-remote-interface": "0.25.5",
    "chrome-unmirror": "0.1.0",
    "extend": "3.0.1",
    "find-up": "2.1.0",
    "globby": "8.0.0",
    "handlebars": "4.0.10",
    "highlight.js": "9.12.0",
    "import-cwd": "2.1.0",
    "jimp": "0.2.28",
    "koa": "2.5.0",
    "koa-favicon": "2.0.1",
    "koa-rewrite": "3.0.0",
    "koa-static": "4.0.2",
    "mkdirp": "0.5.1",
