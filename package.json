{
  "description": "Unified testing framework",
  "author": "QlikTech International AB",
  "private": true,
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist --no-ci",
    "test:node": "aw -c aw.config.js",
    "test:chrome": "aw chrome -c aw.config.js",
    "test:protractor": "aw protractor -c examples/protractor/aw.config.js",
    "test:puppeteer": "aw puppeteer -c examples/puppeteer/aw.config.js",
    "test:coveralls": "aw -c aw.config.js --coverage && cat coverage/lcov.info | coveralls",
    "lint": "lerna run lint",
    "postinstall": "npm run bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:qlik-oss/after-work.js.git"
  },
  "devDependencies": {
    "@after-work.js/cli": "file:commands/cli",
    "@babel/core": "7.0.0-rc.3",
    "@babel/plugin-transform-react-jsx": "7.0.0-rc.2",
    "@babel/preset-env": "7.0.0-rc.4",
    "@babel/register": "7.0.0-rc.2",
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.5",
    "babel-plugin-istanbul": "4.1.6",
    "coveralls": "3.0.2",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "globby": "8.0.1",
    "lerna": "3.1.4",
    "protractor": "5.4.0",
    "requirejs": "2.3.5",
    "typescript": "3.0.1",
    "yargs": "12.0.1"
  }
}
