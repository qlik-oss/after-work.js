    "@types/chai": "4.1.2",
    "@types/mocha": "5.0.0",
    "babel-core": "6.26.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "coveralls": "3.0.0",
    "eslint": "4.19.1",
